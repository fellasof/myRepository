	</field>
	<field name="NOTIONAL">
		<xpath>/ClearingXML/FpML/trade/creditDefaultSwap/protectionTerms/calculationAmount/amount</xpath>
		<type>BigDecimal</type>
	</field>
	<field name="PARTY_ID_SWAPSWIRE">
		<xpath>/ClearingXML/FpML/party[partyId='SWAPSWIRE']/@id</xpath>
		<type>String</type>
	</field>
	<field name="PARTY_CLIENT">
		<xpath>/ClearingXML/FpML/party</xpath>
		<type>String</type>
	</field>
