	</field>
	<field name="REF_OBLIG_CODE">
		<xpath>/ClearingXML/FpML/trade/creditDefaultSwap/generalTerms/referenceInformation/referenceObligation/bond/instrumentId[@instrumentIdScheme='http://www.fpml.org/spec/2003/instrument-id-RED-pair-1-0']</xpath>
