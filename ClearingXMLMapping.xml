<mapping>
	<field name="PARTY_TRADE_IDENTIFIER">
		<xpath>/ClearingXML/FpML/trade/tradeHeader/partyTradeIdentifier</xpath>
		<type>String</type>
	</field>
	<field name="CB_PARTY_REFERENCE">
		<xpath>/ClearingXML/swPartyClearingBroker/partyReference/@href</xpath>
		<type>String</type>
	</field>
	<field name="CB_PARTY_ID">
		<xpath>/ClearingXML/swPartyClearingBroker/swClearingBroker/partyId</xpath>
		<type>String</type>
	</field>
	<field name="TMF_BUYER">
		<xpath>/ClearingXML/FpML/trade/creditDefaultSwap/generalTerms/buyerPartyReference/@href</xpath>
		<type>String</type>
	</field>
	<field name="TMF_SELLER">
		<xpath>/ClearingXML/FpML/trade/creditDefaultSwap/generalTerms/sellerPartyReference/@href</xpath>
		<type>String</type>
	</field>
	<field name="IDX_FAMILY_TYPE">
		<xpath>/ClearingXML/FpML/trade/creditDefaultSwap/generalTerms/indexReferenceInformation</xpath>
		<type>String</type>
	</field>
